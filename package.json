{
  "name": "memba-design-system",
  "version": "0.1.0",
  "scripts": {
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "lint": "eslint . --fix --ext ts,tsx",
    "prettier": "prettier \"./src/**/*.{ts,tsx,js,json}\" --check",
    "prettier:fix": "prettier \"./src/**/*.{ts,tsx,js,json}\" --check --write",
    "prepare": "husky install",
    "test": "jest src --silent",
    "release": "standard-version",
    "release-major": "standard-version --release-as major",
    "release-minor": "standard-version --release-as minor",
    "release-patch": "standard-version --release-as patch",
    "pre-release-alpha": "standard-version --prerelease alpha",
    "pre-release-beta": "standard-version --prerelease beta",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build -o deployment/build"
  },
  "dependencies": {
    "@memba-labs/shared": "^0.7.0",
    "@types/node": "20.5.4",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "eslint": "8.47.0",
    "formik": "^2.4.3",
    "next": "^13.4.19",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@jamesacarr/eslint-formatter-github-actions": "^0.2.0",
    "@jest/types": "^29.5.0",
    "@storybook/addon-essentials": "^7.4.0-alpha.0",
    "@storybook/addon-interactions": "^7.4.0-alpha.0",
    "@storybook/addon-links": "^7.4.0-alpha.0",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.4.0-alpha.0",
    "@storybook/react": "^7.4.0-alpha.0",
    "@storybook/testing-library": "^0.2.0",
    "@svgr/webpack": "^8.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.2",
    "@types/styled-components": "^5.1.26",
    "@types/webpack": "^5.28.1",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-unicorn": "^47.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-mock-extended": "^3.0.4",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "standard-version": "^9.5.0",
    "storybook": "^7.4.0-alpha.0"
  }
}
